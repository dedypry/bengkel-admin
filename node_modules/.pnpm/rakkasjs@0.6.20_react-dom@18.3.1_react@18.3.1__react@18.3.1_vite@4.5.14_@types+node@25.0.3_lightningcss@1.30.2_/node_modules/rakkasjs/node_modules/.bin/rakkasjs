#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/elefin/Documents/Project/NEW-PPPI/elefin-backoffice/node_modules/.pnpm/rakkasjs@0.6.20_react-dom@18.3.1_react@18.3.1__react@18.3.1_vite@4.5.14_@types+node@25.0.3_lightningcss@1.30.2_/node_modules/rakkasjs/node_modules:/Users/elefin/Documents/Project/NEW-PPPI/elefin-backoffice/node_modules/.pnpm/rakkasjs@0.6.20_react-dom@18.3.1_react@18.3.1__react@18.3.1_vite@4.5.14_@types+node@25.0.3_lightningcss@1.30.2_/node_modules:/Users/elefin/Documents/Project/NEW-PPPI/elefin-backoffice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/elefin/Documents/Project/NEW-PPPI/elefin-backoffice/node_modules/.pnpm/rakkasjs@0.6.20_react-dom@18.3.1_react@18.3.1__react@18.3.1_vite@4.5.14_@types+node@25.0.3_lightningcss@1.30.2_/node_modules/rakkasjs/node_modules:/Users/elefin/Documents/Project/NEW-PPPI/elefin-backoffice/node_modules/.pnpm/rakkasjs@0.6.20_react-dom@18.3.1_react@18.3.1__react@18.3.1_vite@4.5.14_@types+node@25.0.3_lightningcss@1.30.2_/node_modules:/Users/elefin/Documents/Project/NEW-PPPI/elefin-backoffice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
